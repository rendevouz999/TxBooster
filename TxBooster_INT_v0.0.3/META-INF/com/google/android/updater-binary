#!/sbin/sh
# Simple updater-binary wrapper untuk Magisk
# Memanggil updater-script

PACKAGE="$1"   # path zip
ARCH="$2"
OUTFD="$3"

ui_print() {
  echo "ui_print $1" > /proc/self/fd/$OUTFD
}

ui_print("Installing TxBooster_INT v0.0.3...")
sh $PACKAGE/META-INF/com/google/android/updater-script
ui_print("Installation Complete.")
